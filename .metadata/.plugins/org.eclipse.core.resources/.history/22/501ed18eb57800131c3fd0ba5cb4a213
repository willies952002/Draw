package tk.willies952002.java.bukkitPlugins.Draw;

import java.util.Iterator;

public enum ColorValues {
	WHITE(0),
	ORANGE(1),
	MAGENTA(2),
	LIGHT_BLUE(3),
	YELLOW(4),
	LIME(5),
	PINK(6),
	GRAY(7),
	SILVER(8),
	CYAN(9),
	PURPLE(10),
	BLUE(11),
	BROWN(12),
	GREEN(13),
	RED(14),
	BLACK(15);

	int dmgValue = 0;
	
	ColorValues(int damage) {
		this.dmgValue = damage;
	}
	
	public ColorValues getByID(int id) {
		switch( id ) {
			case 0: return WHITE;
			default: return WHITE;
		}
	}

}
